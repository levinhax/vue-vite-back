{
  "name": "vue-vite-back",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:stage": "vite build --mode staging",
    "lint:eslint": "eslint --fix --ext \"src/**/*.{vue,tsx}\"",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"src/**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "lint-staged": {
    "src/**/*.{vue, tsx}": [
      "npm run lint:eslint"
    ],
    "src/**/*.{js,json,tsx,css,less,scss,vue,html,md}": [
      "npm run lint:prettier"
    ],
    "src/**/*.{vue,less,postcss,css,scss}": [
      "npm run lint:stylelint"
    ]
  },
  "dependencies": {
    "ant-design-vue": "^2.0.0-rc.5",
    "typescript": "^4.1.3",
    "vue": "^3.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "@vue/compiler-sfc": "^3.0.4",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-vue": "^7.3.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "sass": "^1.30.0",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "vite": "^1.0.0-rc.13"
  }
}
